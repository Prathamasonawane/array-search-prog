include<stdio.h>
int main()
{
        int i,n,a[100],s=0;
        printf("enter limit :");
        scanf("%d",&n);
        printf("enter n numbers :");
        for(i=0;i<n;i++)
        {
        scanf("%d",&a[i]);
        }
        for(i=0;i<n;i++)
        {
        if(a[i]%2!=0)
        s=s+a[i];
        }
        printf("sum of odd no =%d",s);
        }
